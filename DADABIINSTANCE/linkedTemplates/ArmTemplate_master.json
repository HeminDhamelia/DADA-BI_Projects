{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DADABIINSTANCE"
		},
		"Snowflake1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Snowflake1'"
		},
		"Snowflake2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Snowflake2'"
		},
		"Snowflake_Seattle_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Snowflake_Seattle'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"AzureDataLakeStorage_LakeService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dadabiproj.dfs.core.windows.net/"
		},
		"AzureKeyVault_LinkService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DADABIKEY.vault.azure.net/"
		},
		"BLOB__STORAGE_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'BLOB__STORAGE'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DADABIINSTANCE_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Snowflake1_connectionString": {
						"value": "[parameters('Snowflake1_connectionString')]"
					},
					"Snowflake2_connectionString": {
						"value": "[parameters('Snowflake2_connectionString')]"
					},
					"Snowflake_Seattle_connectionString": {
						"value": "[parameters('Snowflake_Seattle_connectionString')]"
					},
					"linkedService1_connectionString": {
						"value": "[parameters('linkedService1_connectionString')]"
					},
					"AzureDataLakeStorage_LakeService_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_LakeService_properties_typeProperties_url')]"
					},
					"AzureKeyVault_LinkService_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_LinkService_properties_typeProperties_baseUrl')]"
					},
					"BLOB__STORAGE_sasUri": {
						"value": "[parameters('BLOB__STORAGE_sasUri')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}